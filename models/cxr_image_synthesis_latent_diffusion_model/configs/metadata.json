        "outputs": {
            "pred": {
                "type": "image",
                "format": "magnitude",
                "modality": "CXR",
                "num_channels": 1,
                "spatial_shape": [
                    512,
                    512
                ],
                "dtype": "float32",
                "value_range": [
                    0,
                    1
